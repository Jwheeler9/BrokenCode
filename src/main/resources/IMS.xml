<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">
	<bean name="StateAbbrv" class="cerberus.IMS.beans.StateAbbrv">
		
	</bean>
	<bean name="state" class="cerberus.IMS.beans.StateAbbrv">
		<property name="stateAbbrv" value="NY"/>
		<property name="stateName" value="New York"/>
	</bean>
	<bean name="Address" class="cerberus.IMS.beans.Address"  autowire="byName">
		
	</bean>
	<bean name="Client" class="cerberus.IMS.beans.Client">
		
	</bean>
	<bean name="ClientType" class="cerberus.IMS.beans.ClientType">
		
	</bean>
	<bean name="CompositeKey" class="cerberus.IMS.beans.CompositeKey">
	
	</bean>
	<bean name="PoLine" class="cerberus.IMS.beans.ClientType">
		
	</bean>
	<bean name="Product" class="cerberus.IMS.beans.Product">
		
	</bean>
	<bean name="ProductCategory" class="cerberus.IMS.beans.ProductCategory">
		
	</bean>
	<bean name="PurchaseOrder" class="cerberus.IMS.beans.PurchaseOrder">
		
	</bean>
</beans>
